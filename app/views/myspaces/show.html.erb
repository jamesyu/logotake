<% title "#{h @logo.text} - MySpace Logo" %>

<% content_for :facebook_url do %>
    <meta property="og:url" content="<%= myspace_url(@logo) %>" />
<% end %>

<%= render :partial => 'layouts/sub_header' %>

<div class="logo_container">
    <%= render @logo %>
</div>

<% jsblock do %>
    LT.centerMyspace();
<% end %>


<div class="share">
    <div id="fb-root"></div><script src="http://connect.facebook.net/en_US/all.js#appId=166368633374864&amp;xfbml=1"></script><fb:comments width="425"></fb:comments>
</div>
